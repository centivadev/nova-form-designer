{
    "name": "eom-plus/nova-form-designer",
    "description": "A Laravel Nova 4 visual form designer",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Sebastien CRAMATTE",
            "email": "oss@operativeit.es",
            "homepage": "https://operativeit.es"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "laravel/nova": "^4.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "EomPlus\\NovaFormDesigner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EomPlus\\NovaFormDesigner\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
